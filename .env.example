# Server UDP port (QUIC)
HYTALE_PORT=5520

# JVM memory (tune for your machine)
JAVA_OPTS=-Xms1G -Xmx4G

# Strongly recommended during plugin dev
HYTALE_DISABLE_SENTRY=true

# Set true only if you actually use early plugins
HYTALE_ACCEPT_EARLY_PLUGINS=false

# Any extra server args you want appended
# HYTALE_EXTRA_ARGS=--backup --backup-frequency 30
